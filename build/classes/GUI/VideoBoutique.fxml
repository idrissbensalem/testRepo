<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="523.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.VideoBoutiqueController">
   <children>
      <MediaView fx:id="mediaview" fitHeight="300.0" fitWidth="600.0" layoutY="6.0">
         <viewport>
            <Rectangle2D />
         </viewport>
      </MediaView>
      <Button fx:id="btnPlay" layoutX="100.0" layoutY="329.0" mnemonicParsing="false" onAction="#play" text="Play" />
      <Button fx:id="btnPause" layoutX="268.0" layoutY="329.0" mnemonicParsing="false" onAction="#pause" text="Pause" />
      <Button fx:id="btnReset" layoutX="440.0" layoutY="329.0" mnemonicParsing="false" onAction="#reset" text="Reset" />
   </children>
</AnchorPane>
